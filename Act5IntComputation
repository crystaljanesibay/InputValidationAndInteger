import 'dart:io';

void main() {
    
    print("Enter first number: ");
    int num1 = int.parse(stdin.readLineSync()!);
    print("Enter second number: ");
    int num2 = int.parse(stdin.readLineSync()!);

  print('\nResult:');
  print('$num1 + $num2 = The sum is ${num1 + num2}');
  print('$num1 - $num2 = The diff is ${num1 - num2}');
  print('$num1 * $num2 = The mul is ${num1 * num2}');
  print('$num1 / $num2 = The div is ${num1 / num2}');
}
