
import 'dart:io';

void main() {
    print(" === Age Checker ===\n");

    print("Enter a Number");
    int? inputNumber = int.parse(stdin.readLineSync()!);

    print("The entered number is ${inputNumber}");
}

if (inputNumber != null) {
    if (inputNumber >= 18) {
        print("you're an adult");
    }
    else {
        print("you're a minor");
    }
    else {
        print("Please provide a valid age");
    }
}
